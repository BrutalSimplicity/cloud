{
  "name": "koreo-gcloud-types",
  "version": "1.0.0",
  "description": "Some TypeScript definitions for Google Cloud services",
  "main": "tests.ts",
  "scripts": {
    "pretest": "npm-run-all --sequential ts:build gcloud:datastore-start gcloud:pubsub-start",
    "test": "npm-run-all --continue-on-error --sequential mocha:test gcloud:datastore-shutdown gcloud:pubsub-shutdown",
    "mocha:test": "mocha --timeout 20000 --require ts-node/register tests/*",
    "mocha:datastore": "mocha --timeout 20000 --require ts-node/register tests/datastore.ts",
    "mocha:logging": "mocha --require ts-node/register tests/logging.ts",
    "mocha:pubsub": "mocha --timeout 20000 --require ts-node/register tests/pubsub.ts",
    "gcloud:datastore-start": "pm2 --silent start scripts/startDatastoreServer.js --name local-datastore",
    "gcloud:datastore-shutdown": "pm2 --silent stop local-datastore",
    "gcloud:pubsub-start": "pm2 --silent start scripts/startPubSub.js --name local-pubusb",
    "gcloud:pubsub-shutdown": "pm2 --silent  stop local-pubusb",
    "ts:build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrutalSimplicity/cloud.git"
  },
  "author": "Kory Taborn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrutalSimplicity/cloud/issues"
  },
  "homepage": "https://github.com/BrutalSimplicity/cloud#readme",
  "dependencies": {},
  "devDependencies": {
    "@google-cloud/datastore": "^1.0.0",
    "@google-cloud/logging": "^1.0.0",
    "@google-cloud/pubsub": "^0.11.0",
    "@types/chai": "^3.5.1",
    "@types/chalk": "^0.4.31",
    "@types/dateformat": "^1.0.1",
    "@types/mocha": "^2.2.41",
    "@types/request": "0.0.42",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "dateformat": "^2.0.0",
    "request": "^2.81.0"
  }
}
